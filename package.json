{
  "name": "vulugonew",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "deploy": "npm run build:web && firebase deploy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/services",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "jest --testPathPattern=__tests__/e2e",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:update-snapshots": "jest --updateSnapshot",
    "test:all": "chmod +x ./scripts/test.sh && ./scripts/test.sh all",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "validate": "npm run lint && npm run type-check && npm run test:ci",
    "clean": "rm -rf node_modules && rm -rf .expo && npm install",
    "clean:cache": "expo r -c",
    "analyze": "npm run fix-deps && npm audit",
    "prebuild": "npm run validate",
    "fix-deps": "npx expo install --fix",
    "postinstall": "patch-package"
  },
  "dependencies": {
